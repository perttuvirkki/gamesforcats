{
  "expo": {
    "name": "GamesForCats",
    "slug": "GamesForCats",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/appicon-iOS-Default-1024x1024@1x.png",
    "scheme": "gamesforcats",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "icon": "./assets/appicon-iOS-Default-1024x1024@1x.png",
      "supportsTablet": true,
      "requireFullScreen": true,
      "bundleIdentifier": "com.ludussuper.gamesforcats",
      "buildNumber": "2",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UISupportedInterfaceOrientations": [
          "UIInterfaceOrientationPortrait"
        ],
        "UISupportedInterfaceOrientations~ipad": [
          "UIInterfaceOrientationPortrait"
        ]
      }
    },
    "android": {
      "versionCode": 8,
      "icon": "./assets/appicon-iOS-Default-1024x1024@1x.png",
      "adaptiveIcon": {
        "backgroundColor": "#FF8A00",
        "foregroundImage": "./assets/appicon-iOS-Default-1024x1024@1x.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.ludussuper.gamesforcats"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/box1.png",
          "imageWidth": 300,
          "resizeMode": "contain",
          "backgroundColor": "#FF8A00",
          "dark": {
            "backgroundColor": "#FF8A00"
          }
        }
      ],
      "expo-localization",
      "expo-audio"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
